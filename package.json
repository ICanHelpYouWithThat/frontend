{
  "name": "ichywt-frontend",
  "description": "Simple site which allows people who are logged in through Facebook to find tasks that help out an organization who needs a skill they can offer - location/time constraints are matching.",
  "version": "0.0.0",
  "author": "Jesse Bowden",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ICanHelpYouWithThat/frontend.git"
  },
  "keywords": [
    "ichywt"
  ],
  "bugs": {
    "url": "https://github.com/ICanHelpYouWithThat/frontend/issues"
  },
  "homepage": "https://github.com/ICanHelpYouWithThat/frontend#readme",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "init": "npm prune && npm install -g typescript && npm install",
    "build": "ng build --target=production --environment=prod",
    "deploy": "./node_modules/.bin/ngc -p ./src/tsconfig.aot.json && ng build --prod --aot",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "dependencies": {
    "@angular/common": "~2.2.3",
    "@angular/compiler": "~2.2.1",
    "@angular/compiler-cli": "^2.2.1",
    "@angular/core": "^2.2.3",
    "@angular/forms": "~2.2.3",
    "@angular/http": "~2.2.3",
    "@angular/material": "^2.0.0-alpha.10",
    "@angular/platform-browser": "~2.2.3",
    "@angular/platform-browser-dynamic": "~2.2.3",
    "@angular/router": "~3.2.3",
    "hammerjs": "^2.0.8",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.11",
    "ts-node": "1.7.0",
    "tslint": "3.0.0",
    "typescript": "^2.0.10",
    "webdriver-manager": "10.2.8"
  }
}
